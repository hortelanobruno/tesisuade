	<queryString><![CDATA[SELECT
     recibos."numero" AS recibos_numero,
     datepart(dd,recibos."fecharendicion")AS dia,
datepart(mm,recibos."fecharendicion")AS mes,
datepart(yy,recibos."fecharendicion")AS anio,
     recibos."beneficiario" AS recibos_beneficiario,
     recibos."monto" AS recibos_monto,
     recibos."estadorecibo" AS recibos_estadorecibo,
     recibos."estadotransaccion" AS recibos_estadotransaccion,
     recibos."motivo" AS recibos_motivo
FROM
     "dbo"."recibos" recibos
WHERE
     (recibos."fecharendicion" between $P{fecha1} AND $P{fecha2}) AND
	recibos."estadorecibo" = 'completada']]></queryString>

