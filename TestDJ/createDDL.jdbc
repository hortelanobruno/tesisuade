CREATE TABLE BRUTO (NOMBRE VARCHAR(255) NOT NULL, PASSWORD VARCHAR(255), NIVEL INTEGER, PROPIETARIO TINYINT(1) default 0, PRIMARY KEY (NOMBRE))
CREATE TABLE PELEA (FECHA DATE NOT NULL, VICTORIA TINYINT(1) default 0, BRUTO_NOMBRE VARCHAR(255), RIVAL_NOMBRE VARCHAR(255), PRIMARY KEY (FECHA))
ALTER TABLE PELEA ADD CONSTRAINT FK_PELEA_BRUTO_NOMBRE FOREIGN KEY (BRUTO_NOMBRE) REFERENCES BRUTO (NOMBRE)
ALTER TABLE PELEA ADD CONSTRAINT FK_PELEA_RIVAL_NOMBRE FOREIGN KEY (RIVAL_NOMBRE) REFERENCES BRUTO (NOMBRE)
