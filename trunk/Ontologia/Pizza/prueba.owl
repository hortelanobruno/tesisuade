      <owl:Class rdf:ID="PrawnTopping"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="TunaTopping"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#SeafoodTopping"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="GreenPepperTopping">
    <rdfs:subClassOf rdf:resource="#PepperTopping"/>
