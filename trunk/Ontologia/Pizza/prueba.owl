            </owl:someValuesFrom>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="hasTopping"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Class rdf:ID="Pizza"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="#VegetableTopping">
