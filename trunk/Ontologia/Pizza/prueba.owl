    <owl:disjointWith>
      <owl:Class rdf:ID="PepperTopping"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="MushroomTopping"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="TomatoTopping"/>
