    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#MushroomTopping"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#OliveTopping"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#TomatoTopping"/>
