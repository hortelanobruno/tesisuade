      <owl:Class rdf:ID="RedPepperTopping"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="#TunaTopping">
    <owl:disjointWith>
      <owl:Class rdf:about="#PrawnTopping"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#AnchovyTopping"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#SeafoodTopping"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#TomatoTopping">
    <rdfs:subClassOf rdf:resource="#VegetableTopping"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#CaperTopping"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#OnionTopping"/>
    <owl:disjointWith rdf:resource="#PepperTopping"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#MushroomTopping"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#OliveTopping"/>
  </owl:Class>
  <owl:Class rdf:about="#Pizza">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasBase"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#PizzaBase"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <owl:disjointWith rdf:resource="#PizzaBase"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#PizzaTopping"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="ProbeInconsistentTopping">
    <rdfs:comment xml:lang="en">This class should be inconsistent when the ontology is classified.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#VegetableTopping"/>
    <rdfs:subClassOf rdf:resource="#CheeseTopping"/>
  </owl:Class>
  <owl:Class rdf:ID="MargheritaPizza">
    <owl:disjointWith>
      <owl:Class rdf:ID="SohoPizza"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="AmericanHotPizza"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="AmericanaPizza"/>
    </owl:disjointWith>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A pizza that only has Mozarella and Tomato
toppings</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#TomatoTopping"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasTopping"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#MozzarellaTopping"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasTopping"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="NamedPizza"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#AmericanHotPizza">
    <rdfs:comment xml:lang="es">A pizza that only has Mozarella, Pepperoni and Tomato
toppings</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#NamedPizza"/>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="#MargheritaPizza"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasTopping"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#TomatoTopping"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#AmericanaPizza"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasTopping"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#PepperoniTopping"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasTopping"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#JalapenoPepperTopping"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="#SohoPizza"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasTopping"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#MozzarellaTopping"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#SohoPizza">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#OliveTopping"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasTopping"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasTopping"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#ParmezanTopping"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="#MargheritaPizza"/>
    <owl:disjointWith rdf:resource="#AmericanHotPizza"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#AmericanaPizza"/>
    </owl:disjointWith>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A pizza that only has Mozarella and Tomato
toppings</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasTopping"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#MozzarellaTopping"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#TomatoTopping"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasTopping"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#NamedPizza"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#CaperTopping">
    <rdfs:subClassOf rdf:resource="#VegetableTopping"/>
    <owl:disjointWith rdf:resource="#OnionTopping"/>
    <owl:disjointWith rdf:resource="#PepperTopping"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#MushroomTopping"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#OliveTopping"/>
    <owl:disjointWith rdf:resource="#TomatoTopping"/>
  </owl:Class>
  <owl:Class rdf:about="#SalamiTopping">
